<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pop2_bg"
    tools:context=".JobsLessonActivity4">

    <Button
        android:id="@+id/button84"
        android:layout_width="100dp"
        android:layout_height="42dp"
        android:layout_marginBottom="360dp"
        android:fontFamily="@font/ranchers"
        android:text="Listen"
        android:textSize="14sp"
        app:backgroundTint="#5B4D75"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.823"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button83"
        android:layout_width="86dp"
        android:layout_height="44dp"
        android:layout_marginBottom="76dp"
        android:fontFamily="@font/ranchers"
        android:onClick="openJobsLessonActivity5"
        android:text="Next"
        app:backgroundTint="#181B55"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView107"
        android:layout_width="137dp"
        android:layout_height="114dp"
        android:layout_marginBottom="472dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/jobslogo" />

    <ImageView
        android:id="@+id/imageView108"
        android:layout_width="215dp"
        android:layout_height="224dp"
        android:layout_marginBottom="264dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/artist" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="326dp"
        android:layout_height="121dp"
        android:layout_marginBottom="136dp"
        android:fontFamily="@font/chewy"
        android:text="Artist: Is a person who creates works of art ( paintings, drawings, films, graphic designs);a professional entertainer (e.g., singer, dancer, actor)   Painter: creates paintings using paint (usually on paper)."
        android:textColor="#F4EBEB"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>